ed_group:[]\r\n\t\t}\r\n\t},\n\tmounted() {\r\n\t\r\n\t\t// this.side_bar_list = this.$store.getters.side_bar\r\n\t\tthis.set_bar()\r\n\t\t// console.log(this.$store.getters.side_bar)\r\n\t},\r\n\tmethods:{\r\n\t\t/*设置路由*/\r\n\t\tset_bar(){\r\n\t\t\t// 深度遍历配置树, 摘取叶子节点作为路由部分\r\n\t\t\tthis.deepTravel(this.$store.getters.side_bar, viewConfig => {\r\n\t\t\t\tconsole.log('viewConfig')\r\n\t\t\t\t// 构造舞台pages路由\r\n\t\t\t\tconst viewRouter = {}\r\n\t\t\t\tviewRouter.link = viewConfig.link\r\n\t\t\t\tviewRouter.side_name = viewConfig.side_name\r\n\t\t\t\tviewRouter.icon = viewConfig.icon\r\n\t\t\t\tviewRouter.children =[]\r\n\t\t\t\tif(viewConfig.children){\r\n\t\t\t\t\tviewConfig.children.forEach(item=>{\r\n\t\t\t\t\t\tviewRouter.children.push(item)\r\n\t\t\t\t\t})\r\n\t\t\t\t}\r\n\t\t\t\tthis.side_bar_list.push(viewRouter)\r\n\t\t\t})\r\n\t\t\t\r\n\t\t\tconsole.log(this.side_bar_list)\r\n\t\t},\r\n\t\t/*遍历*/\r\n\t\tdeepTravel(config, fuc) {\r\n\t\t\tif (Array.isArray(config)) {\r\n\t\t\t\tconfig.forEach(subConfig => {\r\n\t\t\t\t\tthis.deepTravel(subConfig, fuc)\r\n\t\t\t\t})\r\n\t\t\t} else {\r\n\t\t\t\tfuc(config)\r\n\t\t\t} \r\n\t\t},\r\n\t\t/*点击事件*/\r\n\t\tside_active(item){\r\n\t\t\tthis.checked_group.splice(0,this.checked_group.length)\r\n\t\t\tif(this.checked_group.indexOf(item) === -1){\r\n\t\t\t\tthis.checked_group.push(item)\r\n\t\t\t}\r\n\t\t}\r\n\t}\r\n\t\n}\n</script>\n\n<style scoped=\"scoped\" lang=\"scss\">\n.left-bar{\r\n\twidth: 200px;\r\n\t// height: 100%;\r\n\tcolor: $default-word-color;\r\n\tz-index: 1;\r\n\t.logo{\r\n\t\theight: 60px;\r\n\t\tbackground-color: ghostwhite;\r\n\t}\r\n\t.slide-type{\r\n\t\t.type-name{\r\n\t\t\tcursor: pointer;\r\n\t\t\toverflow: hidden;\r\n\t\t\tfont-size: $uni-font-size-paragraph;\r\n\t\t\tline-height: 56px;\r\n\t\t\tbackground: $default-bg-color;\r\n\t\t\tpadding: 0 20px;\r\n\t\t\tposition: relative;\r\n\t\t\ttransition: all 0.3s;\r\n\t\t\t&.active{\r\n\t\t\t\tbackground-color: $active-bg-color;\r\n\t\t\t\t.word{\r\n\t\t\t\t\tcolor: $active-word-color;\r\n\t\t\t\t}\r\n\t\t\t}\r\n\t\t\t&:hover{\r\n\t\t\t\tbackground-color: $active-bg-color;\r\n\t\t\t}\r\n\t\t\t.icon{\r\n\t\t\t\tfloat: left;\r\n\t\t\t\twidth: 16px;\r\n\t\t\t\theight: 16px;\r\n\t\t\t\tmargin:18px 10px 20px 40px ;\r\n\t\t\t}\r\n\t\t\t.word{\r\n\t\t\t\tfont-size: $uni-font-size-paragraph;\r\n\t\t\t\tfloat: left;\r\n\t\t\t}\r\n\t\t\t.row{\r\n\t\t\t\tposition: absolute;\r\n\t\t\t\tright: 6px;\r\n\t\t\t\ttop: 20px;\r\n\t\t\t}\r\n\t\t}\r\n\t\t.sec-item{\r\n\t\t\tbackground: $default-bg-color;\r\n\t\t\t&.active{\r\n\t\t\t\tbackground-color: $active-bg-color;\r\n\t\t\t\tspan{\r\n\t\t\t\t\tcolor: $active-word-color;\r\n\t\t\t\t}\r\n\t\t\t}\r\n\t\t\tspan{\r\n\t\t\t\tcursor: pointer;\r\n\t\t\t\tfont-size: $hk-mid;\r\n\t\t\t\ttext-align: center;\r\n\t\t\t\tpadding: 0 5px 0 20px;\r\n\t\t\t\tposition: relative;\r\n\t\t\t\ttext-indent: 10px;\r\n\t\t\t\tdisplay: block;\r\n\t\t\t\theight: 56px;\r\n\t\t\t\tline-height: 56px;\r\n\t\t\t\ttransition: all 0.3s;\r\n\t\t\t\t&::before{\r\n\t\t\t\t\tcontent: \"\";\r\n\t\t\t\t\twidth: 4px;\r\n\t\t\t\t\theight: 4px;\r\n\t\t\t\t\tborder-radius: 50%;\r\n\t\t\t\t\tborder: 1px solid #c4c9d2;\r\n\t\t\t\t\tposition: absolute;\r\n\t\t\t\t\tleft: 63px;\r\n\t\t\t\t\ttop: 50%;\r\n\t\t\t\t\ttransform: translateY(-50%);\r\n\t\t\t\t}\r\n\t\t\t\t&:hover{\r\n\t\t\t\t\tbackground-color:$active-bg-color;\r\n\t\t\t\t}\r\n\t\t\t\t\r\n\t\t\t}\r\n\t\t}\r\n\t}\r\n\t\r\n}\n</style>\n"]}]}