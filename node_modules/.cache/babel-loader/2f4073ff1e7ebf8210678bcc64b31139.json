AAA,CAAA,IAAA;AACA,QAAA,UAAA,CAAA,SAAA,GAAA,UAAA,CAAA,SAAA;AACA,QAAA,UAAA,CAAA,IAAA,GAAA,UAAA,CAAA,IAAA;AACA,QAAA,UAAA,CAAA,QAAA,GAAA,EAAA;;AACA,YAAA,UAAA,CAAA,QAAA,EAAA;AACA,UAAA,UAAA,CAAA,QAAA,CAAA,OAAA,CAAA,UAAA,IAAA,EAAA;AACA,YAAA,UAAA,CAAA,QAAA,CAAA,IAAA,CAAA,IAAA;AACA,WAFA;AAGA;;AACA,QAAA,KAAA,CAAA,aAAA,CAAA,IAAA,CAAA,UAAA;AACA,OAdA,EAFA,CAiBA;AACA,KApBA;;AAqBA;AACA,IAAA,UAtBA,sBAsBA,MAtBA,EAsBA,GAtBA,EAsBA;AAAA;;AACA,UAAA,KAAA,CAAA,OAAA,CAAA,MAAA,CAAA,EAAA;AACA,QAAA,MAAA,CAAA,OAAA,CAAA,UAAA,SAAA,EAAA;AACA,UAAA,MAAA,CAAA,UAAA,CAAA,SAAA,EAAA,GAAA;AACA,SAFA;AAGA,OAJA,MAIA;AACA,QAAA,GAAA,CAAA,MAAA,CAAA;AACA;AACA,KA9BA;;AA+BA;AACA,IAAA,WAhCA,uBAgCA,IAhCA,EAgCA;AACA,WAAA,aAAA,CAAA,MAAA,CAAA,CAAA,EAAA,KAAA,aAAA,CAAA,MAAA;;AACA,UAAA,KAAA,aAAA,CAAA,OAAA,CAAA,IAAA,MAAA,CAAA,CAAA,EAAA;AACA,aAAA,aAAA,CAAA,IAAA,CAAA,IAAA;AACA,aAAA,MAAA,CAAA,QAAA,CAAA,6BAAA,EAAA,IAAA;AACA,aAAA,OAAA,CAAA,IAAA,CAAA,IAAA,CAAA,IAAA,EAHA,CAIA;AACA,OAPA,CAQA;;AACA;AAzCA;AA/BA,CAAA","sourcesContent":["<template>\r\n\t<!-- 配合点击事件进行节流，将 router-link 跳转 改为 this.$router.push()方法跳转 -->\n\t<div class=\"left-bar\">\r\n\t\t<div class=\"logo\">XXXX</div>\r\n\t\t<swiper ref=\"mySwiper\" :options=\"swiperOption\" >\r\n\t\t\t<swiper-slide class=\"text\">\r\n\t\t\t\t<!-- 无子选项 -->\r\n\t\t\t\t<div \r\n\t\t\t\t:key=\"item.id\"\r\n\t\t\t\tv-for=\"item of this.side_bar_list\">\r\n\t\t\t\t\t<div \r\n\t\t\t\t\t@click=\"side_active(item)\"\r\n\t\t\t\t\tv-if=\"item.children.length==0\"\r\n\t\t\t\t\t:class=\"{'slide-type':true}\">\r\n\t\t\t\t\t\t<!-- <router-link :to=\"item.link\" tag=\"div\" > -->\r\n\t\t\t\t\t\t\t<h1 :class=\"{'type-name':true,'active':checked_group.indexOf(item)>-1}\">\r\n\t\t\t\t\t\t\t\t<div class=\"icon\"><img :src=\"item.icon\" alt=\"\"></div>\r\n\t\t\t\t\t\t\t\t<span class=\"word\">{{item.side_name}}</span>\r\n\t\t\t\t\t\t\t</h1>\r\n\t\t\t\t\t\t<!-- </router-link> -->\r\n\t\t\t\t\t</div>\r\n\t\t\t\t\t<!-- 有子选项 -->\r\n\t\t\t\t\t<div \r\n\t\t\t\t\tv-else\r\n\t\t\t\t\tclass=\"slide-type\">\r\n\t\t\t\t\t\t<h1 class=\"type-name\">\r\n\t\t\t\t\t\t\t<div class=\"icon\"><img :src=\"item.icon\" alt=\"\"></div>\r\n\t\t\t\t\t\t\t<span class=\"word\">{{item.side_name}}</span>\r\n\t\t\t\t\t\t\t<i class=\"el-icon-arrow-down row\"></i>\r\n\t\t\t\t\t\t</h1>\r\n\t\t\t\t\t\t<div \r\n\t\t\t\t\t\t@click=\"throttleSide(item2)\"\r\n\t\t\t\t\t\t:key=\"item2.id\"\r\n\t\t\t\t\t\tv-for=\"item2 of item.children\"\r\n\t\t\t\t\t\t:class=\"{'sec-item':true,'active':checked_group.indexOf(item2)>-1}\">\r\n\t\t\t\t\t\t\t<span>{{item2.side_name}}</span>\r\n\t\t\t\t\t\t\t<!-- <router-link :to=\"item2.link\" tag=\"span\" >{{item2.side_name}}</router-link> -->\r\n\t\t\t\t\t\t</div>\r\n\t\t\t\t\t\t\r\n\t\t\t\t\t</div> \r\n\t\t\t\t</div>\r\n\t\t\t</swiper-slide>\r\n\t\t\t<div class=\"swiper-scrollbar\" slot=\"scrollbar\"></div>\r\n\t\t</swiper>\r\n\t\t\r\n\t\t\r\n\t\t\r\n\t</div>\n</template>\n\n<script>\r\nimport Utils from '@/kun/utils/util'\r\nimport { Swiper, SwiperSlide} from 'vue-awesome-swiper'\r\nimport 'swiper/css/swiper.css'\nexport default {\n\tname: 'SideBar',\r\n\tcomponents: {\r\n\t\tSwiper,\r\n\t\tSwiperSlide\r\n\t},\r\n\tdata(){\r\n\t\treturn {\r\n\t\t\twait: 300, // 300ms之内不能重复发起请求\r\n\t\t\tthrottleSide: null, // 节流点击\r\n\t\t\tside_bar_list:[],\r\n\t\t\tchecked_group:[],\r\n\t\t\tswiperOption: {\r\n\t\t\t\tdirection: 'vertical',\r\n\t\t\t\tslidesPerView: 'auto',\r\n\t\t\t\tfreeMode: true,\r\n\t\t\t\tscrollbar: {\r\n\t\t\t\tel: '.swiper-scrollbar'\r\n\t\t\t\t},\r\n\t\t\t\tmousewheel: true\r\n\t\t\t}\r\n\t\t}\r\n\t},\r\n\tmounted() {\r\n\t\t//节流左侧点击\r\n\t\tthis.throttleSide = Utils.throttle(this.side_active, this.wait)\r\n\t\tconsole.log('this.$store.getters.active_side_bar')\r\n\t\tconsole.log(this.$store.getters.active_side_bar)\r\n\t\t\r\n\t\tthis.set_bar()\r\n\t},\r\n\tmethods:{\r\n\t\t/*设置路由*/\r\n\t\tset_bar(){\r\n\t\t\t// 深度遍历配置树, 摘取叶子节点作为路由部分\r\n\t\t\tthis.deepTravel(this.$store.getters.side_bar, viewConfig => {\r\n\t\t\t\t// console.log('viewConfig')\r\n\t\t\t\t// 构造舞台pages路由\r\n\t\t\t\tconst viewRouter = {}\r\n\t\t\t\tviewRouter.link = viewConfig.link\r\n\t\t\t\tviewRouter.side_name = viewConfig.side_name\r\n\t\t\t\tviewRouter.icon = viewConfig.icon\r\n\t\t\t\tviewRouter.children =[]\r\n\t\t\t\tif(viewConfig.children){\r\n\t\t\t\t\tviewConfig.children.forEach(item=>{\r\n\t\t\t\t\t\tviewRouter.children.push(item)\r\n\t\t\t\t\t})\r\n\t\t\t\t}\r\n\t\t\t\tthis.side_bar_list.push(viewRouter)\r\n\t\t\t})\r\n\t\t\t// console.log(this.side_bar_list)\r\n\t\t},\r\n\t\t/*遍历*/\r\n\t\tdeepTravel(config, fuc) {\r\n\t\t\tif (Array.isArray(config)) {\r\n\t\t\t\tconfig.forEach(subConfig => {\r\n\t\t\t\t\tthis.deepTravel(subConfig, fuc)\r\n\t\t\t\t})\r\n\t\t\t} else {\r\n\t\t\t\tfuc(config)\r\n\t\t\t} \r\n\t\t},\r\n\t\t/*点击事件*/\r\n\t\tside_active(item){\r\n\t\t\tthis.checked_group.splice(0,this.checked_group.length)\r\n\t\t\tif(this.checked_group.indexOf(item) === -1){\r\n\t\t\t\tthis.checked_group.push(item)\r\n\t\t\t\tthis.$store.dispatch('Config/save_active_side_bar',item);\r\n\t\t\t\tthis.$router.push(item.link)\r\n\t\t\t\t// console.log(item.path)\r\n\t\t\t}\r\n\t\t\t// console.log(item)\r\n\t\t},\r\n\t\t\r\n\t}\r\n\t\n}\n</script>\n\n<style scoped=\"scoped\" lang=\"scss\">\n.left-bar{\r\n\twidth: 200px;\r\n\theight: 100%;\r\n\tcolor: $default-word-color;\r\n\tz-index: 1;\r\n\toverflow: hidden;\r\n\t.swiper-container{\r\n\t\theight: calc( 100% - 60px);\r\n\t}\r\n\t.swiper-slide{\r\n\t\theight: auto;\r\n\t}\r\n\t.logo{\r\n\t\theight: 60px;\r\n\t\tbackground-color: ghostwhite;\r\n\t}\r\n\t.slide-type{\r\n\t\t.type-name{\r\n\t\t\tcursor: pointer;\r\n\t\t\toverflow: hidden;\r\n\t\t\tfont-size: $uni-font-size-paragraph;\r\n\t\t\tline-height: 56px;\r\n\t\t\tbackground: $default-bg-color;\r\n\t\t\tpadding: 0 20px;\r\n\t\t\tposition: relative;\r\n\t\t\ttransition: all 0.3s;\r\n\t\t\t&.active{\r\n\t\t\t\tbackground-color: $active-bg-color;\r\n\t\t\t\t.word{\r\n\t\t\t\t\tcolor: $active-word-color;\r\n\t\t\t\t}\r\n\t\t\t}\r\n\t\t\t&:hover{\r\n\t\t\t\tbackground-color: $active-bg-color;\r\n\t\t\t}\r\n\t\t\t.icon{\r\n\t\t\t\tfloat: left;\r\n\t\t\t\twidth: 16px;\r\n\t\t\t\theight: 16px;\r\n\t\t\t\tmargin:18px 10px 20px 40px ;\r\n\t\t\t}\r\n\t\t\t.word{\r\n\t\t\t\tfont-size: $uni-font-size-paragraph;\r\n\t\t\t\tfloat: left;\r\n\t\t\t}\r\n\t\t\t.row{\r\n\t\t\t\tposition: absolute;\r\n\t\t\t\tright: 6px;\r\n\t\t\t\ttop: 20px;\r\n\t\t\t}\r\n\t\t}\r\n\t\t.sec-item{\r\n\t\t\tbackground: $default-bg-color;\r\n\t\t\t&.active{\r\n\t\t\t\tbackground-color: $active-bg-color;\r\n\t\t\t\tspan{\r\n\t\t\t\t\tcolor: $active-word-color;\r\n\t\t\t\t}\r\n\t\t\t}\r\n\t\t\tspan{\r\n\t\t\t\tcursor: pointer;\r\n\t\t\t\tfont-size: $hk-mid;\r\n\t\t\t\ttext-align: center;\r\n\t\t\t\tpadding: 0 5px 0 20px;\r\n\t\t\t\tposition: relative;\r\n\t\t\t\ttext-indent: 10px;\r\n\t\t\t\tdisplay: block;\r\n\t\t\t\theight: 56px;\r\n\t\t\t\tline-height: 56px;\r\n\t\t\t\ttransition: all 0.3s;\r\n\t\t\t\t&::before{\r\n\t\t\t\t\tcontent: \"\";\r\n\t\t\t\t\twidth: 4px;\r\n\t\t\t\t\theight: 4px;\r\n\t\t\t\t\tborder-radius: 50%;\r\n\t\t\t\t\tborder: 1px solid #c4c9d2;\r\n\t\t\t\t\tposition: absolute;\r\n\t\t\t\t\tleft: 63px;\r\n\t\t\t\t\ttop: 50%;\r\n\t\t\t\t\ttransform: translateY(-50%);\r\n\t\t\t\t}\r\n\t\t\t\t&:hover{\r\n\t\t\t\t\tbackground-color:$active-bg-color;\r\n\t\t\t\t}\r\n\t\t\t\t\r\n\t\t\t}\r\n\t\t}\r\n\t}\r\n\t\r\n}\n</style>\n"],"sourceRoot":"src/components/layout"}]}