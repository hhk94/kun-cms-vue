/\" class=\"bc\">\r\n\t\t\t<el-breadcrumb-item>banner相关</el-breadcrumb-item>\r\n\t\t\t<el-breadcrumb-item>banner类别设置</el-breadcrumb-item>\r\n\t\t</el-breadcrumb>\r\n\t\t<div class=\"center\">\r\n\t\t\t\r\n\t\t\t<div class=\"center-item\">\r\n\t\t\t\t<h1 class=\"title\">banner类别添加</h1>\r\n\t\t\t\t<div class=\"input-body\">\r\n\t\t\t\t\tid：\r\n\t\t\t\t\t<el-input\r\n\t\t\t\t\tdisabled\r\n\t\t\t\t\tsuffix-icon=\"el-icon-mouse\"\r\n\t\t\t\t\tv-model=\"id\"\r\n\t\t\t\t\tclearable\r\n\t\t\t\t\t>\r\n\t\t\t\t\t</el-input>\r\n\t\t\t\t</div>\r\n\t\t\t\t<div class=\"input-body\">\r\n\t\t\t\t\t类别名：\r\n\t\t\t\t\t<el-input\r\n\t\t\t\t\tsuffix-icon=\"el-icon-mouse\"\r\n\t\t\t\t\tv-model=\"typename\"\r\n\t\t\t\t\t>\r\n\t\t\t\t\t</el-input>\r\n\t\t\t\t</div>\r\n\t\t\t\t<div class=\"submit\" >\r\n\t\t\t\t\t<el-button  type=\"primary\" plain @click=\"clear()\">清空编辑</el-button>\r\n\t\t\t\t\t<el-button  type=\"primary\" plain @click=\"submit()\">提交/修改</el-button>\r\n\t\t\t\t</div>\r\n\t\t\t\t\r\n\t\t\t</div>\r\n\t\t\t<div class=\"center-item\">\r\n\t\t\t\t<el-table\r\n\t\t\t\t:data=\"banner_list\"\r\n\t\t\t\tstyle=\"width: 100%\"\r\n\t\t\t\tmax-height=\"600\">\r\n\t\t\t\t<el-table-column\r\n\t\t\t\tfixed\r\n\t\t\t\tprop=\"id\"\r\n\t\t\t\tlabel=\"id\"\r\n\t\t\t\twidth=\"50\">\r\n\t\t\t\t</el-table-column>\r\n\t\t\t\t<el-table-column\r\n\t\t\t\tprop=\"typename\"\r\n\t\t\t\tlabel=\"文章类别\"\r\n\t\t\t\twidth=\"\">\r\n\t\t\t\t\t<template slot-scope=\"scope\">\r\n\t\t\t\t\t\t<el-tag\r\n\t\t\t\t\t\ttype=\"primary\"\r\n\t\t\t\t\t\t:effect=\"id==scope.row.id?'dark':'plain' \"\r\n\t\t\t\t\t\tdisable-transitions>{{scope.row.typename}}</el-tag>\r\n\t\t\t\t\t</template>\r\n\t\t\t\t</el-table-column>\r\n\t\t\t\t<el-table-column\r\n\t\t\t\tfixed=\"right\"\r\n\t\t\t\tlabel=\"操作\"\r\n\t\t\t\twidth=\"150\">\r\n\t\t\t\t<template slot-scope=\"scope\">\r\n\t\t\t\t\t<el-button\r\n\t\t\t\t\tsize=\"mini\"\r\n\t\t\t\t\t@click=\"handleEdit(scope.row)\">编辑</el-button>\r\n\t\t\t\t\t<el-button\r\n\t\t\t\t\tsize=\"mini\"\r\n\t\t\t\t\ttype=\"danger\"\r\n\t\t\t\t\t@click=\"handleDelete(scope.row)\">删除</el-button>\r\n\t\t\t\t</template>\r\n\t\t\t\t</el-table-column>\r\n\t\t\t\t</el-table>\r\n\t\t\t</div>\r\n\t\t</div>\r\n\t</div>\n</template>\n\n<script>\r\n// import { Storage } from '@/kun/utils/storage'\r\n// var storage = new Storage();\r\n// import Config from '@/config'\r\nimport Banner from '@/kun/api/banner'\r\nexport default {\n\tname: 'BannerType',\r\n\t\r\n\tdata() {\r\n\t\treturn {\r\n\t\t\tbanner_list:[],\r\n\t\t\tid:'',\r\n\t\t\ttypename:''\r\n\t\t}\r\n\t},\r\n\tmethods: {\r\n\t\tasync banner_type_list_get(){//获取banner类别列表\r\n\t\t\tlet result\r\n\t\t\ttry {\r\n\t\t\t\tresult = await Banner.banner_type_list_get()\r\n\t\t\t} catch (e) {\r\n\t\t\t\tconsole.log(e)\r\n\t\t\t}\r\n\t\t\tif(result.data.state==window.g.SUCCESS_STATE){\r\n\t\t\t\tconsole.log(result)\r\n\t\t\t\tthis.banner_list = result.data.data\r\n\t\t\t}\r\n\t\t},\r\n\t\thandleEdit(row){//点击修改\r\n\t\t\tthis.id = row.id\r\n\t\t\tthis.typename = row.typename\r\n\t\t\tthis.$notify({\r\n\t\t\t\ttitle: '提示',\r\n\t\t\t\tmessage: '请在左侧修改您选中的标签'\r\n\t\t\t});\r\n\t\t},\r\n\t\tasync submit(){//提交banner-type\r\n\t\t\tif(this.typename.length==0){\r\n\t\t\t\tthis.$notify({\r\n\t\t\t\t\ttitle: '错误',\r\n\t\t\t\t\tmessage: '类别名不能为空',\r\n\t\t\t\t\ttype:'error'\r\n\t\t\t\t});\r\n\t\t\t\treturn;\r\n\t\t\t}\r\n\t\t\tlet data = {\r\n\t\t\t\tid:this.id,\r\n\t\t\t\ttypename:this.typename\r\n\t\t\t}\r\n\t\t\tlet result\r\n\t\t\ttry {\r\n\t\t\t\tresult = await Banner.banenr_type_add(data)\r\n\t\t\t} catch (e) {\r\n\t\t\t\tconsole.log(e)\r\n\t\t\t}\r\n\t\t\tif(result.data.state==window.g.SUCCESS_STATE){\r\n\t\t\t\tconsole.log(result)\r\n\t\t\t\tthis.$notify({\r\n\t\t\t\t\ttitle: '提示',\r\n\t\t\t\t\tmessage: result.data.msg,\r\n\t\t\t\t\ttype:'success'\r\n\t\t\t\t});\r\n\t\t\t\tthis.banner_type_list_get()\r\n\t\t\t}\r\n\t\t},\r\n\t\tclear(){//清空左侧编辑数据\r\n\t\t\tthis.id = ''\r\n\t\t\tthis.typename = ''\r\n\t\t},\r\n\t\tasync delete_type(row){\r\n\t\t\tlet data = {\r\n\t\t\t\tid:row.id\r\n\t\t\t}\r\n\t\t\tlet result\r\n\t\t\ttry {\r\n\t\t\t\tresult = await Banner.banner_type_delete(data)\r\n\t\t\t} catch (e) {\r\n\t\t\t\tconsole.log(e)\r\n\t\t\t}\r\n\t\t\tif(result.data.state==window.g.SUCCESS_STATE){\r\n\t\t\t\tthis.$notify({\r\n\t\t\t\t\ttitle: '提示',\r\n\t\t\t\t\tmessage: result.data.msg,\r\n\t\t\t\t\ttype:'success'\r\n\t\t\t\t});\r\n\t\t\t\tthis.banner_type_list_get()\r\n\t\t\t}else{\r\n\t\t\t\tthis.$notify({\r\n\t\t\t\t\ttitle: '提示 - 删除失败',\r\n\t\t\t\t\tmessage: result.data.msg,\r\n\t\t\t\t\ttype:'error'\r\n\t\t\t\t});\r\n\t\t\t}\r\n\t\t},\r\n\t\thandleDelete(row){\r\n\t\t\tthis.$confirm('此操作将永久删除该分类, 是否继续?', '提示', {\r\n\t\t\t\tconfirmButtonText: '确定',\r\n\t\t\t\tcancelButtonText: '取消',\r\n\t\t\t\ttype: 'warning'\r\n\t\t\t\t}).then(() => {\r\n\t\t\t\t\tthis.delete_type(row)\r\n\t\t\t\t\t\r\n\t\t\t\t}).catch(() => {\r\n\t\t\t\t\tthis.$message({\r\n\t\t\t\t\t\ttype: 'info',\r\n\t\t\t\t\t\tmessage: '已取消删除'\r\n\t\t\t\t\t});          \r\n\t\t\t\t});\r\n\t\t\t\r\n\t\t}\r\n\t},\n\tmounted() {\r\n\t\tthis.banner_type_list_get()\r\n\t}\n}\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped lang=\"scss\">\r\n.user{\r\n\t.bc{\r\n\t\t\r\n\t\tmargin: 20px;\r\n\t\twidth: calc(100% - 40px);\r\n\t\tmargin-bottom: 0;\r\n\t}\r\n\t.center{\r\n\t\tdisplay: flex;\r\n\t\t.center-item{\r\n\t\t\twidth: 50%;\r\n\t\t\tpadding: 30px;\r\n\t\t\t.input-body{\r\n\t\t\t\twidth: 50%;\r\n\t\t\t\tmargin: 0 auto;\r\n\t\t\t\ttext-align: left;\r\n\t\t\t\tmargin-bottom: 20px;\r\n\t\t\t\t::v-deep .el-input{\r\n\t\t\t\t\tmargin-top: 10px;\r\n\t\t\t\t}\r\n\t\t\t}\r\n\t\t\t.submit{\r\n\t\t\t\twidth:fit-content;\r\n\t\t\t\tdisplay: flex;\r\n\t\t\t\tmargin: 0 auto;\r\n\t\t\t}\r\n\t\t}\r\n\t\t\r\n\t}\r\n}\n\r\n\r\n// /deep/.markdown-body img{\r\n// \tmax-width: 50%;\r\n// }\n</style>\n"],"sourceRoot":"src/pages/Banner"}]}